{
  "name": "yondako",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https -H local.yondako.com",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "bunx @cloudflare/next-on-pages",
    "preview": "bun pages:build && wrangler pages dev",
    "deploy": "bun pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "generate:schema": "drizzle-kit generate --dialect sqlite --out ./src/db/migrations --schema",
    "svgo": "bunx svgo ./src/assets/icons/"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.4.1",
    "@radix-ui/react-slot": "^1.1.0",
    "cloudflare": "^3.5.0",
    "drizzle-orm": "^0.32.0",
    "htmlparser2": "^9.1.0",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.19",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.4.0",
    "valibot": "^0.36.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@cloudflare/next-on-pages": "^1.12.1",
    "@cloudflare/workers-types": "^4.20240718.0",
    "@svgr/webpack": "^8.1.0",
    "@types/bun": "^1.1.6",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.23.0",
    "eslint": "^9.7.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-next-on-pages": "^1.12.1",
    "lefthook": "^1.7.4",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3",
    "vercel": "^35.1.0",
    "wrangler": "^3.65.1"
  },
  "resolutions": {
    "string-width": "4.2.3"
  }
}
