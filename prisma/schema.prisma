// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Book {
  ndlBibId     String  @id
  title        String
  link         String
  authors      String?
  publisher    String?
  isbn         String?
  jpNo         String?
  thumbnailUrl String?
}
