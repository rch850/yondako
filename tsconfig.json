{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "lib": ["ESNext", "DOM"],
    "types": [
      "vite/client",
      "@cloudflare/workers-types",
      "vite-plugin-svgr/client"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./app",
    "paths": {
      "@/*": ["*"]
    },
    "include": ["**/*.ts", "**/*.tsx"]
  }
}
